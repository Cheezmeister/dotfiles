<?xml version="1.0" encoding="UTF-8"?>
  <keyboard>
    <keybind key="W-p">
      <action name="Execute">
        <command>sh -c 'systemctl suspend &amp; slock'</command>
      </action>
    </keybind>
    <keybind key="W-l">
      <action name="Execute">
        <command>slock</command>
      </action>
    </keybind>
  </keyboard>
